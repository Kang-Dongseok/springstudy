package quiz02;

public class Calculator {

	public static void main(String[] args) {

		// field(property)
		
		// constructor
		
		// method
		public int add(int a, int b) {
			return a + b;
		}
		public int subtract(int a, int b) {
			return a - b;
		}
		public int mutiply(int a, int b) {
			return a + b;
		}
		public double divide(int a, int b) {
			return a + b;
		}

	}

}
